{
    "name": "sietse85/nova-button",
    "description": "(Nova 4+) A Laravel Nova package for adding buttons to your resources.",
    "authors": [
        {
            "name": "Sietse Sietsma",
            "email": "sietse85@gmail.com"
        },
        {
            "name": "Danique Wijnalda",
            "email": "hello@dnwjn.dev"
        },
        {
            "name": "Brian Dillingham",
            "email": "bdillingham88@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "nova",
        "button",
        "nova4",
        "field"
    ],
    "version": "1.0.28",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Sietse85\\NovaButton\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sietse85\\NovaButton\\FieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
